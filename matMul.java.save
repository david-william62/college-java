import java.util.*;
class matrix {
 public static void main(String[] args) {
  Scanner in = new Scanner(System.in);
  System.out.println("Enter row and column size");
  int r = nextInt();
  int c = nextInt();
  int i,j,k;
  int a[][] = new int[r][c];
  int b[][] = new int[r][c];
  System.out.println("Enter values of first matrix");
  for(i=0;i<r;i++) {
   for(j=0;j<c;j++) {
    a[i][j]=in.nextInt();
   }
  }
  System.out.println("Enter values of second matrix");
  for(i=0;i<r;i++) {
   for(j=0;j<c;j++) {
    b[i][j]=in.nextInt();
   }
  }
  int c[][] = new int[r][c];
  for(i=0;i<r;i++) {
   for(j=0;j<c;j++) {
    c[i][j]=0;
    for(k=0;k<c;k++) {
     c[i][j] += a[i][k] * b[k][j];
    }
   }
  }
        int[][] result = new int[r][c];
        for (int i = 0; i < r; i++) {
            for (int j = 0; j < c; j++) {
                result[i][j] = 0;
                for (int k = 0; k < c; k++) {
                    result[i][j] += a[i][k] * b[k][j];
                }
            }
        }
 }
}
